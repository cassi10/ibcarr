{
  "name": "ibcarr",
  "version": "0.0.0",
  "description": "ibcarr monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build --scope=\"@ibcarr/*\" --ignore=\"functions\"",
    "export": "turbo run export --scope=\"@ibcarr/*\"",
    "dev": "turbo run dev --no-cache --continue --scope=\"@ibcarr/*\"",
    "lint": "turbo run lint --continue",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "author": "cassi10",
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*",
    "apps/*",
    "firebase/*"
  ],
  "dependencies": {
    "eslint": "^8.6.0",
    "eslint-formatter-pretty": "^4.1.0",
    "prettier": "^2.5.1",
    "turbo": "^1.0.24"
  },
  "packageManager": "npm@8.3.1"
}
